{
    "root": true,
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
    },
    "extends": "airbnb",
    "plugins": [
        "react"
    ],
    "rules": {
        "semi": [
            0
        ],
        "react/jsx-filename-extension": [
            0
        ],
        "react/prefer-stateless-function": [
            0
        ],
        "react/require-default-props": [
            0
        ],
        "arrow-body-style": [
            0
        ],
        "no-param-reassign": [
            0
        ],
        "import/first": [
            0
        ]
    },
    "settings": {
        "import/resolver": {
            "node": {
                "paths": [
                    "src"
                ]
            }
        }
    }
}